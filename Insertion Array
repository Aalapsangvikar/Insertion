#include <stdio.h>
int main()
{
int a[100],pos,val;
int n=7,i;
printf("Enter the array elements :");
for(i=0;i<n;i++)
scanf("%d",&a[i]);
printf("Enter the position of element to be inserted :");
scanf("%d",&pos);
printf("Enter the vslue :");
scanf("%d",&val);
for(i=n;i>=pos;i--)
a[i+1]=a[i];
a[pos]=val;
for(i=0;i<=n;i++)
printf("%d",a[i]);
return 0;
}
